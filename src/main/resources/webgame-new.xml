<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!--mysql-connector jar包地址 绝对路径  -->
    <classPathEntry location="D:/lmn_code/mysql-connector-java-5.1.47.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="UTF-8"/>
        <commentGenerator type="com.lmn.code.MyCommentGenerator">
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>
        
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://wanatestdb-public.rwlb.rds.aliyuncs.com:3306/web_game?characterEncoding=utf-8"
                        userId="testdb" password="Wana123456">
        </jdbcConnection>
        <javaTypeResolver type="com.lmn.code.MyJavaTypeResolverDefaultImpl">
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- do targetPackage 包路径  targetProject 项目名/路径  同一workspace下   -->
        <javaModelGenerator targetPackage="com.wanatech.services.webgame.model"
                            targetProject="D:/idea-workspace/wana-new/webgame/server/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"
                         targetProject="D:/idea-workspace/wana-new/webgame/server/src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wanatech.services.webgame.dao"
                             targetProject="D:/idea-workspace/wana-new/webgame/server/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
