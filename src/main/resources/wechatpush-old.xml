                            targetProject="D:/idea-workspace/git-download/wechatpush/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
       
        <sqlMapGenerator targetPackage="mappers"
