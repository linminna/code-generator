            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wana.eventPushConfig.mapper"
                             targetProject="D:/idea-workspace/git-download/wechatpush/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table schema="t_event_push_config" tableName="t_event_push_config" domainObjectName="EventPushConfig">
            <property name="useActualColumnNames" value="false"/>
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        
    </context>

</generatorConfiguration>